<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ffc8c8c8" android:endColor="#ffc8c8c8" android:angle="0.0" android:centerY="0.75" android:centerColor="#ffc8c8c8" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ff00a0ff" android:endColor="#ff00a0ff" android:angle="0.0" android:centerY="0.75" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ff00a0ff" android:endColor="#ff00a0ff" android:angle="0.0" />
            </shape>
        </clip>
    </item>
</layer-list>